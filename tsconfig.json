{
    "compilerOptions": {
      "outDir": "./js-generated",
      "rootDir": ".",
      "module": "commonjs",
      "target": "es6",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "declaration": true,
      "declarationDir": "./.d.ts"
    },
    "include": [
      "**/*.ts"
    ],
    "exclude": [
      "node_modules",
      "js-generated"
    ]
}